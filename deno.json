{
  "name": "@whyk/greeting",
  "version": "0.1.22",
  "exports": {
    ".": "./main.ts",
    "./core": "./packages/core/mod.ts",
    "./cli": "./packages/cli/mod.ts"
  },
  "tasks": {
    "dev": "deno run main.ts",
    "lint": "deno run -A scripts/lint-jsr.ts",
