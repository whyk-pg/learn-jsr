    "update": "deno run -A scripts/create-jsr-config.ts"
  },
  "imports": {
    "@std/cli": "jsr:@std/cli@^0.224.6",
    "@std/assert": "jsr:@std/assert@0.217"
  },
  "exclude": ["README.md"]
}
